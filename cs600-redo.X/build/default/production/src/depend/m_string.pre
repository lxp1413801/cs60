uint16_t HEX8(uint8_t* hex,uint8_t x)
{
*hex++=HexTable[(x&0xf0)>>4];
*hex++=HexTable[(x&0x0f)];
hex='\0';
return 3;
}


uint16_t m_str_b2h(uint8_t* h,uint8_t* b,uint16_t len)
{
uint16_t i=0;
uint8_t t8;
for(i=0;i<len;i++)
{
t8=*b++;
*h++=HexTable[(t8&0xf0)>>4];
*h++=HexTable[(t8&0x0f)];
}
return (len<<1);
}

