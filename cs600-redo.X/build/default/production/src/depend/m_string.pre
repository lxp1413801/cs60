void m_int_2_str(uint8_t* buf,int32_t x,uint8_t len)
{
int32_t t;
if(!len)return;

t=m_math_pow(10,len);
if(x>=t-1)x=t-1;
while(len--)
{
t=x%10;
*buf--=HexTable[t];
x/=10;
}
}


